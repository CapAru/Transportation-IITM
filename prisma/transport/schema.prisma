generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/.prisma/client-transport"
}

datasource db {
  provider = "postgresql"
  url      = env("TRANSPORT_DATABASE_URL")
}

model gps_data {
  id              Int       @id @default(autoincrement())
  imei            String?
  date            DateTime? @db.Date
  time            DateTime? @db.Time(6)
  latitude        Float?
  longitude       Float?
  speed           Float?
  signal_strength Float?
}

model wifi_data {
  id        Int       @id @default(autoincrement())
  timestamp DateTime? @db.Timestamp(6)
  rssi      Float?
  mac       String?
  device_id String?
}

model obu_data {
  id        Int       @id @default(autoincrement())
  log_time  DateTime? @db.Timestamp(6)
  pkt_hdr   String?
  rsu_id    Int?
  obu_id    Int?
  pkt_type  Int?
  pkt_len   Int?
  time_ist  DateTime? @db.Time(6)
  gps_valid String?   @db.Char(1)
  latitude  Float?
  longitude Float?
  date_ist  DateTime? @db.Date
  accel_x   Float?
  accel_y   Float?
  accel_z   Float?
  gyro_x    Float?
  gyro_y    Float?
  gyro_z    Float?
}

model rsu_data {
  id        Int       @id @default(autoincrement())
  log_time  DateTime? @db.Timestamp(6)
  pkt_hdr   String?
  rsu_id    Int?
  obu_id    Int?
  pkt_type  Int?
  pkt_len   Int?
  time_ist  DateTime? @db.Time(6)
  gps_valid String?   @db.Char(1)
  latitude  Float?
  longitude Float?
  date_ist  DateTime? @db.Date
  accel_x   Float?
  accel_y   Float?
  accel_z   Float?
  gyro_x    Float?
  gyro_y    Float?
  gyro_z    Float?
}
